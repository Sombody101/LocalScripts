  box-sizing: border-box;
  border-radius: 100%;
  border-left: 20px solid #1aae1e;
  transform: rotate(-30deg);
  display: none;
}
.tentacle-animation {
  animation: tentacle 12s cubic-bezier(0.645, 0.045, 0.355, 1) infinite;
}
.small-tentacle {
  position: absolute;
  left: -16px;
  top: 5px;
  height: 50px;
  width: 50px;
  border-left: 10px solid #1aae1e;
  border-radius: 100%;
  transform: rotate(25deg);
  z-index: 2;
  display: none;
}
