<style type="text/css">
  .flipX {
  animation: 1s anim-flipX ease infinite;
}
@keyframes anim-flipX {
  0% {
    opacity: 0;
    transform: rotateX(90def);
  }
  50% {
    opacity: 1;
    transform: rotateX(720deg);
  }
  100% {
    /* animate nothing to pause animation at the end */
    opacity: 1;
    transform: rotateX(720deg);
  }
}

.block--custom-code {
  margin-top: 0!important;
}
</style>

<script>
const mainLogo = document.querySelector("#block-themekit-content > article > div.paragraph.paragraph--type--header-banner-video-hero.paragraph--view-mode--default.internal-link-cta > div.paragraph-content > div.field.field--name-field-media.field--type-entity-reference.field--label-hidden.field__item > article > div.field.field--name-field-media-svg.field--type-svg-image-field.field--label-hidden.field__item > img");
let logoCounter = 0;
mainLogo.addEventListener('dblclick', () => {
  if (logoCounter === 0) {
    logoCounter += 1
    mainLogo.classList.add('flipX');

    setTimeout(() => {
      mainLogo.classList.remove('flipX');
      logoCounter -= 1;
    }, "1000")
  }
})
</script></div>
      
