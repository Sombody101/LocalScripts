  transform-origin: 0% 100% 0;
  z-index:  0;
}
.ship-animation {
  animation: ship 12s cubic-bezier(0,0,.58,1) infinite;
}
.ship-rotate {
  position: absolute;
  height: 22px;
  transform: rotate(0deg);
}
.ship-rotate-animation {
  animation: ship-rotate 12s cubic-bezier(.65,.04,.55,.85) infinite;
}  
