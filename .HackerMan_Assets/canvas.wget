<script>
const target = document.querySelector("#block-themekit-content > article > div.node__content > div:nth-child(1) > div > div > div.block.block-layout-builder.block-inline-blockpathways.block--pathways.children-number--3 > div > div > div:nth-child(2) > div > div.field.field--name-field-media.field--type-entity-reference.field--label-hidden.field__item > article > div.field.field--name-field-media-svg.field--type-svg-image-field.field--label-hidden.field__item");
const ship = document.querySelector(".ship");
const shipRotate = document.querySelector(".ship-rotate");
const tentacle  = document.querySelector(".tentacle");
const smallTentacle = document.querySelector(".small-tentacle");
const flagLogo = document.querySelector(".flag-logo");
const flag = document.querySelector(".flag");
const hiddenClock = document.createElement('div');
hiddenClock.classList.add('hidden-clock');
let counter = 0;
hiddenClock.addEventListener('click', () => {
  if (counter === 0) {
    counter += 1
    ship.classList.add('ship-animation');
    shipRotate.classList.add('ship-rotate-animation');
    tentacle.classList.add('tentacle-animation');
    smallTentacle.classList.add('small-tentacle-animation');
    flag.classList.add('flag-animation');
    flagLogo.classList.add('flag-logo-animation');

    setTimeout(() => {
      ship.classList.remove('ship-animation');
      shipRotate.classList.remove('ship-rotate-animation');
      tentacle.classList.remove('tentacle-animation');
      smallTentacle.classList.remove('small-tentacle-animation');
      flag.classList.remove('flag-animation');
      flagLogo.classList.remove('flag-logo-animation');
      counter -= 1;
    }, "12000")
  }
})
target.style.position = 'relative';
target.appendChild(hiddenClock);
document.addEventListener("DOMContentLoaded", () => {
  flag.style.display = 'inline';
  tentacle.style.display = 'inline';
  smallTentacle.style.display = 'inline';
});
</script></div>
      
